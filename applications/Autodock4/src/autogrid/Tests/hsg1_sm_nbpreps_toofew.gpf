parameter_file AD4.1_bound.dat      # force field default parm file
npts 4 4 4                          # num.grid points in xyz
gridfld hsg1_sm.maps.fld            # grid_data_file
spacing 0.375                       # spacing(A)
receptor_types A C HD N NA OA SA    # receptor atom types
ligand_types A C HD N NA OA         # ligand atom type
receptor hsg1_sm.pdbqt              # macromolecule
gridcenter 2.5 6.5 -7.5             # xyz-coordinates or auto
smooth 0.5                          # store minimum energy w/in rad(A)
map hsg1_sm.A.map                   # atom-specific affinity map
map hsg1_sm.C.map                   # atom-specific affinity map
map hsg1_sm.HD.map                  # atom-specific affinity map
map hsg1_sm.N.map                   # atom-specific affinity map
map hsg1_sm.NA.map                  # atom-specific affinity map
map hsg1_sm.OA.map                  # atom-specific affinity map
elecmap hsg1_sm.e.map               # electrostatic potential map
dsolvmap hsg1_sm.d.map              # desolvation potential map
dielectric -1                       # <0, distance-dep.diel;>0, constant
nbp_r_eps 1.5 10.0 12 6 OA          # missing 6th parameter
